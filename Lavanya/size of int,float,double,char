#include <iostream>

using namespace std;

int main()
{
    char charType;
    int integerType;
    float floatType;
    double doubleType;
    
    cout<<"Size of char : "<<sizeof(charType)<<" byte"<<endl;
    cout<<"Size of int : "<<sizeof(integerType)<<" bytes"<<endl;
    cout<<"Size of float : "<<sizeof(floatType)<<" bytes"<<endl;
    cout<<"Size of double : "<<sizeof(doubleType)<<" bytes"<<endl;
    
    return 0;
}
